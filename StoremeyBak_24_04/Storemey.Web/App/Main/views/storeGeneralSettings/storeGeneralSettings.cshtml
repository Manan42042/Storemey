
<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin">General settings</h4>
    @*<div ncy-breadcrumb class="pull-right"></div>*@
</div>
<!-- end: BREADCRUMB -->
<!-- start: BASIC EXAMPLE -->
<div class="container-fluid container-fullw" ng-controller="app.views.storeGeneralSettings">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-white">
                <div class="panel-body">
                    <h3 class="over-title margin-bottom-15">Edit general settings for POS and e-commarce.</h3>
                    <div class="row margin-top-30">
                        <form role="form" id="frmCountry" name="myForm">
                            <div class="col-lg-12 col-md-12">
                                <div class="panel panel-white" style="background-color: #fff;">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <fieldset>
                                                    <legend>
                                                        Settings-1
                                                    </legend>
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Total outlet
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetstoreGeneralSettingsOutputDto.totalOutlets" id="exampleInputEmail1" placeholder="enter total outlet" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Total register
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetstoreGeneralSettingsOutputDto.totalResiters" id="exampleInputPassword1" placeholder="enter total register" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Total users
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetstoreGeneralSettingsOutputDto.totalUsers" id="exampleInputEmail1" placeholder="enter total users" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Max data storage
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetstoreGeneralSettingsOutputDto.maxFileStorage" id="exampleInputPassword1" placeholder="enter max data storage" required>
                                                    </div>
                                                </fieldset>
                                            </div>
                                            <div class="col-md-6">
                                                <fieldset>
                                                    <legend>
                                                        Settings-2
                                                    </legend>
                                                    <div class="form-group">
                                                        <div class="checkbox clip-check check-primary">
                                                            <input type="checkbox" id="checkbox1" value="1" ng-model="GetstoreGeneralSettingsOutputDto.masterRecordEntry" >
                                                            <label for="checkbox1">
                                                                Allow master record entry
                                                            </label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="panel-footer">
                                        <button class="btn btn-primary btn-wide" type="submit" ng-disabled="myForm.$invalid" ng-click="saveAddEdit(this)">
                                            Save
                                        </button>

                                        @*<button class="btn btn-default btn-wide" type="button" ng-click="cancelAddEdit()">
                                            Back
                                        </button>*@
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>