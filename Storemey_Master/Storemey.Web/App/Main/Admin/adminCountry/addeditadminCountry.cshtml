
@{
    string EmailPattern = @"[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$";

}


<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin">Tags</h4>
</div>
<!-- end: BREADCRUMB -->
<!-- start: BASIC EXAMPLE -->
<div class="container-fluid container-fullw" ng-controller="app.views.adminCountry">
    <div class="row">
        <div class="col-md-12">
            <fieldset>
                <legend>
                    Add/Update Of, Tag!
                </legend>
                <div class="panel panel-white">
                    <div class="panel-body">
                        @*<h3 class="over-title margin-bottom-15">Add/Update Of, Tag!</h3>*@

                        <div class="row">
                            <form role="form" id="frmCountry" name="myForm" ng-submit="register(myForm)" ng-validate="validationOptions">
                                <div>
                                    <div class="panel panel-white" style="background-color: #fbecb5;">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Name
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetadminCountryOutputDto.name" name="tagName" placeholder="enter tag name">
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Code
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetadminCountryOutputDto.code" name="description" placeholder="enter description">
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Dail Code
                                                        </label>
                                                        <textarea type="text" class="form-control" ng-model="GetadminCountryOutputDto.dail_Code" placeholder="enter note" name="note"></textarea>
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Currency Name
                                                        </label>
                                                        <textarea type="text" class="form-control" ng-model="GetadminCountryOutputDto.currency_Name" placeholder="enter note" name="note"></textarea>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Curreny Symbol
                                                        </label>
                                                        <textarea type="text" class="form-control" ng-model="GetadminCountryOutputDto.curreny_Symbol" placeholder="enter note" name="note"></textarea>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Current Code
                                                        </label>
                                                        <textarea type="text" class="form-control" ng-model="GetadminCountryOutputDto.current_Code" placeholder="enter note" name="note"></textarea>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Flagimage
                                                        </label>
                                                        <textarea type="text" class="form-control" ng-model="GetadminCountryOutputDto.flagimage" placeholder="enter note" name="note"></textarea>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel-footer">
                                            <button class="btn btn-primary btn-wide" type="submit" ng-disabled="myForm.$invalid" ng-click="saveAddEdit(this)">
                                                Save
                                            </button>

                                            <button class="btn btn-default btn-wide" type="button" ng-click="cancelAddEdit()">
                                                Back
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>
