
@{
    string EmailPattern = @"[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$";

}


<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin">Currancies</h4>
</div>
<!-- end: BREADCRUMB -->
<!-- start: BASIC EXAMPLE -->
<div class="container-fluid container-fullw" ng-controller="app.views.adminCurrancy">
    <div class="row">
        <div class="col-md-12">
            <fieldset>
                <legend>
                    Add/Update Of, Currancy!
                </legend>
                <div class="panel panel-white">
                    <div class="panel-body">
                     
                        <div class="row">
                            <form role="form" id="frmCountry" name="myForm" ng-submit="register(myForm)" ng-validate="validationOptions">
                                <div>
                                    <div class="panel panel-white" style="background-color: #fbecb5;">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Currancy Name
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetadminCurrancyOutputDto.currancy_Name" id="exampleInputEmail1" placeholder="enter name" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Currency Symbol
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetadminCurrancyOutputDto.currency_Symbol" id="exampleInputPassword1" placeholder="enter code" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Currency Code
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetadminCurrancyOutputDto.currency_Code" id="exampleInputEmail1" placeholder="enter daily code" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Currency Code02
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetadminCurrancyOutputDto.currency_Code02" id="exampleInputPassword1" placeholder="enter currency name" required>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel-footer">
                                            <button class="btn btn-primary btn-wide" type="submit" ng-disabled="myForm.$invalid" ng-click="saveAddEdit(this)">
                                                Save
                                            </button>

                                            <button class="btn btn-default btn-wide" type="button" ng-click="cancelAddEdit()">
                                                Back
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>
