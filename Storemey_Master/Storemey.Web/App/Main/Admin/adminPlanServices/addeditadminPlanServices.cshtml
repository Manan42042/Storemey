
@{
    string EmailPattern = @"[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$";

}


<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin">Plan services</h4>
</div>
<!-- end: BREADCRUMB -->
<!-- start: BASIC EXAMPLE -->
<div class="container-fluid container-fullw" ng-controller="app.views.adminPlanServices">
    <div class="row">
        <div class="col-md-12">
            <fieldset>
                <legend>
                    Add/Update Of, Plan service!
                </legend>
                <div class="panel panel-white">
                    <div class="panel-body">

                        <div class="row">
                            <form role="form" id="frmCountry" name="myForm" ng-submit="register(myForm)" ng-validate="validationOptions">
                                <div>
                                    <div class="panel panel-white" style="background-color: #fbecb5;">
                                        <div class="panel-body">

                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Plan name
                                                        </label>
                                                        <ui-select ng-model="GetadminPlanServicesOutputDto.uiplanId" name="country" theme="select2" ng-disabled="" style="min-width: 100%;" title="Choose a person">
                                                            <ui-select-match placeholder="Select a plan">{{$select.selected.planName}}</ui-select-match>
                                                            <ui-select-choices repeat="person in Plans | propsFilter: {planName: $select.search}">
                                                                <div ng-bind-html="person.planName | highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                        @*<input type="text" class="form-control" ng-model="GetadminPlanServicesOutputDto.stateName" name="stateName" placeholder="enter state name">*@
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Service Name
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetadminPlanServicesOutputDto.serviceName" id="exampleInputEmail1" placeholder="enter name" required>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel-footer">
                                            <button class="btn btn-primary btn-wide" type="submit" ng-disabled="myForm.$invalid" ng-click="saveAddEdit(this)">
                                                Save
                                            </button>

                                            <button class="btn btn-default btn-wide" type="button" ng-click="cancelAddEdit()">
                                                Back
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>
