
@{
    string EmailPattern = @"[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$";

}


<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin">Payment Types</h4>
</div>
<!-- end: BREADCRUMB -->
<!-- start: BASIC EXAMPLE -->
<div class="container-fluid container-fullw" ng-controller="app.views.storePaymentTypes">
    <div class="row">
        <div class="col-md-12">
            <fieldset>
                <legend>
                    Add/Update Of, Payment type!
                </legend>
                <div class="panel panel-white">
                    <div class="panel-body">
                        <div class="row">
                            <form role="form" id="frmCountry" name="myForm" ng-submit="register(myForm)" ng-validate="validationOptions">
                                <div>
                                    <div class="panel panel-white" style="background-color: #fbecb5;">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-md-6">

                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Name
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetstorePaymentTypesOutputDto.name" name="name" placeholder="enter name">
                                                    </div>

                                                    <div class="form-group">
                                                        <div class="checkbox clip-check check-primary">
                                                            <input type="checkbox" id="isPOSPaymentType" value="1" ng-model="GetstorePaymentTypesOutputDto.isPOSPaymentType">
                                                            <label for="isPOSPaymentType">
                                                                Is POS Payment Type
                                                            </label>
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <div class="checkbox clip-check check-primary">
                                                            <input type="checkbox" id="isEcommarcePaymentType" value="1" ng-model="GetstorePaymentTypesOutputDto.isEcommarcePaymentType">
                                                            <label for="isEcommarcePaymentType">
                                                                Is Ecommarce Payment Type
                                                            </label>
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Note
                                                        </label>
                                                        <textarea type="text" class="form-control" ng-model="GetstorePaymentTypesOutputDto.note" placeholder="enter note" name="note"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel-footer">
                                            <button class="btn btn-primary btn-wide" type="submit" ng-disabled="myForm.$invalid" ng-click="saveAddEdit(this)">
                                                Save
                                            </button>

                                            <button class="btn btn-default btn-wide" type="button" ng-click="cancelAddEdit()">
                                                Back
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>
