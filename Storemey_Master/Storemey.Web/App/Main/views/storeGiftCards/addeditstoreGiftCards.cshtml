
@{
    string EmailPattern = @"[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$";

}


<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin">GiftCards</h4>
</div>
<!-- end: BREADCRUMB -->
<!-- start: BASIC EXAMPLE -->
<div class="container-fluid container-fullw" ng-controller="app.views.storeGiftCards">
    <div class="row">
        <div class="col-md-12">
            <fieldset>
                <legend>
                    Add/Update Of, GiftCard!
                </legend>
                <div class="panel panel-white">
                    <div class="panel-body">

                        <div class="row">
                            <form role="form" id="frmCountry" name="myForm" ng-submit="register(myForm)" ng-validate="validationOptions">
                                <div>
                                    <div class="panel panel-white" style="background-color: #fbecb5;">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-md-6">

                                                    <div class="form-group">
                                                        <label>
                                                            Customer Name
                                                        </label>
                                                        <ui-select ng-model="GetstoreGiftCardsOutputDto.uiCustomerId" theme="select2" ng-disabled="" style="min-width: 100%;" title="Choose a customer">
                                                            <ui-select-match placeholder="Select a customer">{{$select.selected.customerName}}</ui-select-match>
                                                            <ui-select-choices repeat="person in Customers | propsFilter: {customerName: $select.search}">
                                                                <div ng-bind-html="person.customerName | highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                        <input type="text" ng-model="GetstoreGiftCardsOutputDto.uiCustomerId" style="visibility:hidden;height:0px;padding: 0 !important;" name="country" class="form-control" placeholder="Enter your outlel">
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Giftcard number
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetstoreGiftCardsOutputDto.giftcardNumber" name="giftCardName" placeholder="enter giftCard name">
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Total amount
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetstoreGiftCardsOutputDto.totalAmount" name="giftCardName" placeholder="enter giftCard name">
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Note
                                                        </label>
                                                        <textarea type="text" class="form-control" ng-model="GetstoreGiftCardsOutputDto.note" placeholder="enter note" name="note"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel-footer">
                                            <button class="btn btn-primary btn-wide" type="submit" ng-disabled="myForm.$invalid" ng-click="saveAddEdit(this)">
                                                Save
                                            </button>

                                            <button class="btn btn-default btn-wide" type="button" ng-click="cancelAddEdit()">
                                                Back
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>
