
@{
    string EmailPattern = @"[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$";

}


<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin">Variant value</h4>
</div>
<!-- end: BREADCRUMB -->
<!-- start: BASIC EXAMPLE -->
<div class="container-fluid container-fullw" ng-controller="app.views.storeProductVariantValues">
    <div class="row">
        <div class="col-md-12">
            <fieldset>
                <legend>
                    Add/Update Of, Variant value!
                </legend>
                <div class="panel panel-white">
                    <div class="panel-body">
                        <div class="row ">
                            <form role="form" id="frmCountry" name="myForm" ng-submit="register(myForm)" ng-validate="validationOptions">
                                @*<div class="col-lg-12 col-md-12">*@
                                    <div class="panel panel-white" style="background-color: #fbecb5;">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-md-6">

                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Variant Name
                                                        </label>
                                                        <ui-select ng-model="GetstoreProductVariantValuesOutputDto.uivariantId" name="variant" theme="select2" ng-disabled="" style="min-width: 100%;" title="Choose a variant">
                                                            <ui-select-match placeholder="Select a variant">{{$select.selected.variantName}}</ui-select-match>
                                                            <ui-select-choices repeat="person in Variants | propsFilter: {variantName: $select.search}">
                                                                <div ng-bind-html="person.variantName | highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                        @*<input type="text" class="form-control" ng-model="GetstoreProductVariantValuesOutputDto.variantName" name="stateName" placeholder="enter state name">*@
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">
                                                            Variant Value
                                                        </label>
                                                        <input type="text" class="form-control" ng-model="GetstoreProductVariantValuesOutputDto.variantValue" name="variantValue" placeholder="enter variant value">
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">
                                                            Note
                                                        </label>
                                                        <textarea type="text" class="form-control" ng-model="GetstoreProductVariantValuesOutputDto.note" placeholder="enter note" name="note"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel-footer">
                                            <button class="btn btn-primary btn-wide" type="submit" ng-disabled="myForm.$invalid" ng-click="saveAddEdit(this)">
                                                Save
                                            </button>

                                            <button class="btn btn-default btn-wide" type="button" ng-click="cancelAddEdit()">
                                                Back
                                            </button>
                                        </div>
                                    </div>
                                @*</div>*@
                            </form>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>
