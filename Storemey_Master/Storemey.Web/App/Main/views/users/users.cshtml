
@{
    ViewBag.Title = "Users";
}

<!-- start: BREADCRUMB -->
<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin" translate="">User Management</h4>

    <div ncy-breadcrumb class="pull-right"></div>
</div>
<!-- end: BREADCRUMB -->
<!-- start: RESPONSIVE TABLE -->
<div class="container-fluid container-fullw" ng-controller="UsersCtrl">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-white">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="col-md-3 col-sm-6">
                                <h5 class="over-title margin-bottom-15"> <span class="text-bold">User listing</span></h5>
                            </div>
                            <div class="col-md-9 col-sm-6">
                                <span class="help-inline "  style="float:right">
                                    <select class="cs-select cs-skin-elastic">
                                        <option value="" selected>ACTION PANEL</option>
                                        <option value="france">Export CSV</option>
                                        <option value="brazil">Export EXEL</option>
                                        <option value="argentina">Export PDF</option>
                                        <option value="south-africa">IMPORT DATA</option>
                                    </select>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="padding: 20px;">
                        <div class="col-md-12">
                            <div class="col-md-5 col-sm-6">
                                &nbsp;
                            </div>
                            <div class="col-md-3 col-sm-6" >
                                <input type="daterange" class="form-control" ng-model="dates2" ranges="ranges"  />
                            </div>
                            <div class="col-md-2 col-sm-6">
                                <input type="text" placeholder="Search" id="form-field-7" class="form-control">
                            </div>
                            <span class="help-inline col-md-1 col-sm-6">
                                <button class="btn btn-primary" type="submit">
                                    Search
                                </button>
                            </span>

                            <span class="help-inline col-md-1 col-sm-6">
                                <button class="btn btn-o btn-primary" type="submit">
                                    Add New
                                </button>
                            </span>
                        </div>
                    </div>
                    <div class="table-responsive">
                        <div ui-grid="gridOptions"
                             ui-grid-auto-resize
                             ui-grid-move-columns
                             ui-grid-exporter
                             ui-grid-pagination
                             ui-grid-selection
                             ui-grid-pinning class="grid" style="min-height:385px;background-color: white !important;"></div>

                        <div class="loadmore">
                            <div ng-show="loaderMore" ng-class="result">
                                <img src="~/Content/ng-loader.gif" />
                                {{lblMessage}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end: RESPONSIVE TABLE -->


<div class="row">
    <div class="col-md-12">


