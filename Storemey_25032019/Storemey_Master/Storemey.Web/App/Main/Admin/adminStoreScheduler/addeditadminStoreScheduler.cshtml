
@{
    string EmailPattern = @"[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$";

}


<div class="breadcrumb-wrapper">
    <h4 class="mainTitle no-margin">Store Schedulers</h4>
</div>
<!-- end: BREADCRUMB -->
<!-- start: BASIC EXAMPLE -->
<div class="container-fluid container-fullw" ng-controller="app.views.adminStoreScheduler">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-white">
                <div class="panel-body">
                    <h5 class="over-title margin-bottom-15">Add/Update Of, Store Scheduler!</h5>
                    @*<p>
                            Individual form controls automatically receive some global styling.
                        </p>*@
                    <div class="row margin-top-30">
                        <form role="form" id="frmCountry" name="myForm" ng-submit="register(myForm)" ng-validate="validationOptions">
                            <div class="col-lg-12 col-md-12">
                                <div class="panel panel-white" style="background-color: #fff;">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-md-6">

                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        Store Name
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.storeName" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        Sent Email
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.sentEmail" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        From Email
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.fromEmail" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        To Email
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.toEmail" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        Service Name
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.serviceName" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        Start Date
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.startDate" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        End Date
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.endDate" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        Status
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.status" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        Frequency
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.frequency" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        Recur Week Days
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.recurWeekDays" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="exampleInputEmail1">
                                                        RecurHours
                                                    </label>
                                                    <input type="text" class="form-control" ng-model="GetadminStoreSchedulerOutputDto.recurHours" id="exampleInputEmail1" placeholder="enter name" required>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel-footer">
                                        <button class="btn btn-primary btn-wide" type="submit" ng-disabled="myForm.$invalid" ng-click="saveAddEdit(this)">
                                            Save
                                        </button>

                                        <button class="btn btn-default btn-wide" type="button" ng-click="cancelAddEdit()">
                                            Back
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
